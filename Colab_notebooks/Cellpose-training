        "  nuclear_channel = 2\n",
        "\n",
        "if Nuclear_channel == \"Red\":\n",
        "  nuclear_channel = 1\n",
        "\n",
        "if Nuclear_channel == \"None\":\n",
        "  nuclear_channel = 0\n",
        "\n",
        "if QC_model == \"Cytoplasm\":\n",
        "  channels=[segment_channel,nuclear_channel]\n",
        "\n",
        "if QC_model == \"Cytoplasm2_Omnipose\":\n",
        "  channels=[segment_channel,nuclear_channel]\n",
        "\n",
